						Assignment 12
						    CIS 11

ExitProcess proto
WriteInt64 proto
ReadInt64 proto

.data
myArray QWORD 10d, 20d, 30d, 40d
finalArray QWORD 4 DUP(0)

.code
main proc
	mov r15, 8
	mov rcx, 4
	mov rbx, 0
L1:
	mov rax, rbx
	mul r15
	
	mov r13, rax
	mov rax, myArray[r13]
	
	call WriteInt64
	inc rbx
loop L1

	mov r15, 8
	mov rcx, 4
	mov rbx, 0

WRAPLOOP:
	mov rax, rbx
	mul r15
	
	mov r13, rax
	mov rax, r13
	add rax, 8
	
	mov r14, myArray[r13]
	mov finalArray[rax], r14
	
	mov r14, myArray[r13]
	mov finalArray[0], r14
	add rbx, 1
loop WRAPLOOP
	call ReadInt 64
	
	mov r15, 8
	mov rcx, 4
	mov rbx, 0
	
PRINTLOOP:
	mov rax, rbx
	mul r15
	
	mov r13, rax
	mov rax, finalArray[r13]
	
	call WriteInt64
	inc rbx
loop PRINTLOOP
	call ReadInt64
	
	call ExitProcess
main endp
end
